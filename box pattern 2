#include <stdio.h>

int main(void) {
	// your code goes here
    int n;
    scanf("%d",&n);
    int i,j;
    int count=0,num=n-1;
   for(i=1;i<=2*n-1;i++)
    {
       ++num;
      for(j=0;j<n+1;j++)
       {
          if(i==1||i==n+2)
           {
              printf("%d",n);
           }
          else
           {
              if(j==n-1)
              {
                printf("%d",++count);
               }
              else
               {
                 printf("%d",num);
               }
        }
    }
      
         printf("\n");
   }
	return 0;
}
