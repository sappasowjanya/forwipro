#include <stdio.h>
int main(void) {
	// your code goes here
     int n;
      scanf("%d",&n);
     int s;
     scanf("%d",&s);
    int i,j;
    int count=s;
     for(i=1;i<=n;i++)
     {
        for(j=s;j<=count;j++)
        {
            printf("%d",count);
        }
      count++;
    printf("\n");
    }
 count--;
for(i=n-1;i>=0;i--)
  {
    for(j=s;j<=count;j++)
    {
         printf("%d",count);
    }
   count--;
  printf("\n");
  }
     return 0;
}
