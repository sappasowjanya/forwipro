 *****
******


#include <stdio.h>

int main(void) {
	// your code goes here
    int n;
    scanf("%d",&n);
    int i,j,k;
    for(i=n-1;i>=0;i--)
     {
       for(j=0;j<i;j++)
        {
            printf(" ");
        }
    for(k=i;k<=n-1;k++)
    {
     printf("*");
   }
      printf("\n");
   }
	return 0;
}
