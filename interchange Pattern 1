1*2*3*4
9*10*11*12
13*14*15*16
5*6*7*8








#include <stdio.h>

int main(void) {
	// your code goes here
    int n;
    scanf("%d",&n);
    int i,j,count=0;
    for(i=0;i<n;i++)
     {
       if(i==1)
       {
         count+=n;
       }
       if(i==n-1)
       {
         count=n;
        }
     for(j=0;j<n;j++)
      {
        count++;
        if(j==n-1)
         {
           printf("%d",count);
         }
        else
          {
           printf("%d*",count);
         }
      }
   printf("\n");
  }
	return 0;
}
